package votes;

public class Voters {
	
	String votersName;
	int votersId;
	int voterAge;
	
	public Voters(String votersName, int votersId, int voterAge){
        this.votersName=votersName;
        this.votersId=votersId;
        this.voterAge=voterAge;
	}
	
	
	 public String getvotersName() {
	        return votersName;
	    }

	    public void setvotersName(String votersName) {
	        this.votersName = votersName;
	    }

	    public int getvotersId() {
	        return votersId;
	    }

	    public void setvotersId(int votersId) {
	        this.votersId = votersId;
	    }

	    public int getvoterAge() {
	        return voterAge;
	    }

	    public void setvoterAge(int voterAge) {
	        this.voterAge = voterAge;
	    }

	
	    public void printVoterInfo(){

	        System.out.println(" Voter Name:" + votersName + "\n Voters Id:" + votersId + "\n Age :" + voterAge);
	    }
}
